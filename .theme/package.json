{
  "name": "@0x-jerry/vitepress-theme-blog",
  "description": "My Blog",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/blog.git"
  },
  "bugs": {
    "url": "https://github.com/0x-jerry/blog/issues"
  },
  "type": "module",
  "homepage": "https://github.com/0x-jerry/blog#readme",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsup",
    "release": "x-release",
    "dev": "tsup --watch vitepress"
  },
  "tsup": {
    "entry": [
      "vitepress/config.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "dependencies": {
    "@0x-jerry/unocss-preset-brand": "^0.1.4",
    "@0x-jerry/utils": "^3.0.0",
    "@0x-jerry/vue-kit": "^2.1.0",
    "@iconify-json/carbon": "^1.1.37",
    "@iconify-json/logos": "^1.1.44",
    "@mdit/plugin-figure": "^0.13.0",
    "@rollup/plugin-yaml": "^4.1.2",
    "@vueuse/core": "^10.11.1",
    "cheerio": "^1.0.0",
    "dayjs": "^1.11.12",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "heti": "^0.9.4",
    "i18next": "^23.12.3",
    "i18next-vue": "^5.0.0",
    "less": "^4.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pagefind": "^1.1.0",
    "reading-time": "^1.5.0",
    "sass": "^1.77.8",
    "unocss": "^0.62.1",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.0",
    "vitepress": "^1.3.2",
    "vue": "^3.4.37"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^2.2.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.2.0",
    "@types/nprogress": "^0.2.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  }
}