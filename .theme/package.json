{
  "name": "@0x-jerry/vitepress-theme-blog",
  "description": "My Blog",
  "version": "1.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/blog.git"
  },
  "bugs": {
    "url": "https://github.com/0x-jerry/blog/issues"
  },
  "type": "module",
  "homepage": "https://github.com/0x-jerry/blog#readme",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsup",
    "release": "x-release",
    "dev": "tsup --watch vitepress"
  },
  "tsup": {
    "entry": [
      "vitepress/config.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "dependencies": {
    "@0x-jerry/utils": "^2.1.2",
    "@rollup/plugin-yaml": "^4.1.2",
    "@vite-pwa/vitepress": "^0.4.0",
    "@vueuse/core": "^10.9.0",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "heti": "^0.9.4",
    "i18next": "^23.11.1",
    "i18next-vue": "^4.0.0",
    "less": "^4.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "reading-time": "^1.5.0",
    "sass": "^1.74.1",
    "unocss": "^0.59.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.8",
    "vite-plugin-vue-devtools": "^7.0.25",
    "vitepress": "1.1.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@0x-jerry/x-release": "^1.0.0",
    "@iconify-json/carbon": "^1.1.31",
    "@iconify-json/logos": "^1.1.42",
    "@types/markdown-it": "^14.0.0",
    "@types/node": "^20.12.6",
    "@types/nprogress": "^0.2.3",
    "dayjs": "^1.11.10",
    "pagefind": "^1.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4"
  }
}