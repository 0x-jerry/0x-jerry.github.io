{
  "name": "vitepress-theme-blog",
  "description": "Vitepress Blog",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0x-jerry/blog.git"
  },
  "bugs": {
    "url": "https://github.com/0x-jerry/blog/issues"
  },
  "type": "module",
  "homepage": "https://github.com/0x-jerry/blog#readme",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "default": "./dist/config.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch vitepress"
  },
  "tsup": {
    "entry": [
      "vitepress/config.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true,
    "clean": true
  },
  "dependencies": {
    "@0x-jerry/utils": "^2.0.0",
    "@iconify/json": "^2.2.114",
    "@intlify/unplugin-vue-i18n": "^1.0.1",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^1.3.1",
    "dayjs": "^1.11.9",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3",
    "heti": "^0.9.4",
    "less": "^4.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "reading-time": "^1.5.0",
    "sass": "^1.66.1",
    "shiki": "^0.14.4",
    "unocss": "^0.55.7",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vitepress": "1.0.0-rc.12",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.1",
    "@types/node": "^20.6.0",
    "@types/nprogress": "^0.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
